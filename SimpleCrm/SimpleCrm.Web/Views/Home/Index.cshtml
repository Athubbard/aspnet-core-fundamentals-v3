@model SimpleCrm.Web.Models.HomePageViewModel
@{
}

<html>
<body>
    <h1>@Model.CurrentMessage</h1>

    <table>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
        </tr>

        @foreach (var customer in @Model.Customers)
         {
        <tr>
            <td>@customer.Id</td>
            <td>@customer.FirstName</td>
            <td>@customer.LastName</td>
            <td>@customer.PhoneNumber</td>
        </tr>
         }
         
       

    </table>
</body>
</html>